{
  "expo": {
    "name": "Betty - Office Genius",
    "slug": "betty-office-genius",
    "owner": "tinashelorenzi",
    "scheme": "betty",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
