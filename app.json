      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.betty.officegenius",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "betty-oauth",
            "CFBundleURLSchemes": [
              "betty"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.betty.officegenius",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "betty"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "googleClientId": "1092345143344-jv2v1ke2e01f4k73peuhjuq5kn5ocr6j.apps.googleusercontent.com",
      "apiBaseUrl": "http://10.0.0.166:8000",
      "eas": {
        "projectId": "b793f9b9-eb2a-44c3-a453-418d29d6249f"
      }
    }
  }
